<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:skos="http://www.w3.org/2004/02/skos/core#"
	xmlns:owl="http://www.w3.org/2002/07/owl#">
  <rdf:Description rdf:about="urn:us:gov:ic:cvenum:ism:complieswith#CVEnumISMCompliesWith">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <dc:title>(U) ismcomplies:CVEnumISMCompliesWith definition</dc:title>
    <skos:hasTopConcept rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#USGov"/>
    <skos:hasTopConcept rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#USIC"/>
    <skos:hasTopConcept rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#USDOD"/>
    <skos:hasTopConcept rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#OtherAuthority"/>
    <skos:hasTopConcept rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#USA-CUI-ONLY"/>
    <skos:hasTopConcept rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#USA-CUI"/>
  </rdf:Description>
  <rdf:Description rdf:about="urn:us:gov:ic:cvenum:ism:complieswith#CVEnumISMCompliesWithValues">
    <owl:oneOf>USGov</owl:oneOf>
    <owl:oneOf>USIC</owl:oneOf>
    <owl:oneOf>USDOD</owl:oneOf>
    <owl:oneOf>OtherAuthority</owl:oneOf>
    <owl:oneOf>USA-CUI-ONLY</owl:oneOf>
    <owl:oneOf>USA-CUI</owl:oneOf>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
  </rdf:Description>
  <rdf:Description rdf:about="urn:us:gov:ic:cvenum:ism:complieswith#USGov">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#CVEnumISMCompliesWith"/>
    <skos:notation>USGov</skos:notation>
    <skos:prefLabel>Document claims compliance with all rules encoded in ISM for documents produced by the US Federal Government. This is the minimum set of rules for US documents to adhere to, and all US documents should claim compliance with USGov. For example, a US Intelligence Community document should claim ism:compliesWith="USGov USIC".</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="urn:us:gov:ic:cvenum:ism:complieswith#USIC">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#CVEnumISMCompliesWith"/>
    <skos:notation>USIC</skos:notation>
    <skos:prefLabel>Document claims compliance with all rules encoded in ISM for documents produced by the US Intelligence Community. Documents that claim compliance with USIC MUST also claim compliance with USGov.</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="urn:us:gov:ic:cvenum:ism:complieswith#USDOD">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#CVEnumISMCompliesWith"/>
    <skos:notation>USDOD</skos:notation>
    <skos:prefLabel>Document claims compliance with all rules encoded in ISM for documents produced by the US Department of Defense. Documents that claim compliance with USDOD MUST also claim compliance with USGov.</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="urn:us:gov:ic:cvenum:ism:complieswith#OtherAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#CVEnumISMCompliesWith"/>
    <skos:notation>OtherAuthority</skos:notation>
    <skos:prefLabel>Document claims compliance with an authority other than the USGov, USIC, or USDOD. This token is not allowed if the ism:ownerProducer contains USA.</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="urn:us:gov:ic:cvenum:ism:complieswith#USA-CUI-ONLY">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#CVEnumISMCompliesWith"/>
    <skos:notation>USA-CUI-ONLY</skos:notation>
    <skos:prefLabel>Document is UNCLASSIFIED and was created in an UNCLASSIFIED environment. Document claims compliance with CUI rules but with no USGOV rules for classified environments. Special transforms are required to move the document into a classified environment or to commingle content from this document with any content created in a classified environment.</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="urn:us:gov:ic:cvenum:ism:complieswith#USA-CUI">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="urn:us:gov:ic:cvenum:ism:complieswith#CVEnumISMCompliesWith"/>
    <skos:notation>USA-CUI</skos:notation>
    <skos:prefLabel>Document contains both NSI (National Security Information) and CUI according to DoD rules for CUI. Document claims compliance with 1. CUI rules according to DoD, and 2. USGOV and/or USDOD rules for classified environments.</skos:prefLabel>
  </rdf:Description>
</rdf:RDF>