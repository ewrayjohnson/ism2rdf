@prefix icid: <urn:us:gov:ic:id#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

skos:inScheme a owl:ObjectProperty .

icid:identifier a rdf:Property,
        owl:ObjectProperty ;
    rdfs:comment "Attribute to hold the IC-ID of the data object." ;
    rdfs:range icid:IcIdType .

<urn:us:gov:ic:s:IC-ID:IC-ID> a owl:Ontology .

