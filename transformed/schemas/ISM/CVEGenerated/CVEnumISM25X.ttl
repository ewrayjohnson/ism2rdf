skos:inScheme a owl:ObjectProperty .

ism25x:25X1 a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "25X1" ;
    skos:prefLabel "Reveal the identity of a confidential human source, a human intelligence source, a relationship with an intelligence or security service of a foreign government or international organization, or a non-human intelligence source; or impair the effectiveness of an intelligence method currently in use, available for use, or under development." .

ism25x:25X1-EO-12951 a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "25X1-EO-12951" ;
    skos:prefLabel "\"25X1, EO 12951\" (prescribed by the DNI for use on information described in E.O. 12951, Release of Imagery Acquired by Space-Based National Intelligence Reconnaissance Systems)" .

ism25x:25X2 a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "25X2" ;
    skos:prefLabel "Reveal information that would assist in the development, production, or use of weapons of mass destruction." .

ism25x:25X3 a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "25X3" ;
    skos:prefLabel "Reveal information that would impair U.S. cryptologic systems or activities." .

ism25x:25X4 a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "25X4" ;
    skos:prefLabel "Reveal information that would impair the application of state-of-the-art technology within a U.S. weapon system." .

ism25x:25X5 a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "25X5" ;
    skos:prefLabel "Reveal formally named or numbered U.S. military war plans that remain in effect, or reveal operational or tactical elements of prior plans that are contained in such active plans;" .

ism25x:25X6 a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "25X6" ;
    skos:prefLabel "Reveal information, including foreign government information, that would cause serious harm to relations between the United States and a foreign government, or to ongoing diplomatic activities of the United States" .

ism25x:25X7 a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "25X7" ;
    skos:prefLabel "Reveal information that would impair the current ability of United States Government officials to protect the President, Vice President, and other protectees for whom protection services, in the interest of the national security, are authorized." .

ism25x:25X8 a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "25X8" ;
    skos:prefLabel "Reveal information that would seriously impair current national security emergency preparedness plans or reveal current vulnerabilities of systems, installations, or infrastructures relating to the national security." .

ism25x:25X9 a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "25X9" ;
    skos:prefLabel "Violate a statute, treaty, or international agreement that does not permit the automatic or unilateral declassification of information at 25 years." .

ism25x:50X1 a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "50X1" ;
    skos:prefLabel "The ISCAP has authorized use of this code in the FBI’s classification guidance (which results in a 75-year classification period) for any agency sourcing/reusing the information." .

ism25x:50X1-HUM a skos:Concept ;
    skos:inScheme ism25x:CVEnumISM25X ;
    skos:notation "50X1-HUM" ;
    skos:prefLabel "When the information clearly and demonstrably could be expected to reveal the identity of a confidential human source or a human intelligence source." .

ism25x:50X2 a skos:Concept ;
