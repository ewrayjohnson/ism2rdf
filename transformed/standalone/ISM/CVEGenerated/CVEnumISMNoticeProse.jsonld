    {
      "@id": "ismnoticeprose:RAWFISA_00001",
      "@type": "skos:Concept",
      "skos:inScheme": {
        "@id": "ismnoticeprose:CVEnumISMNoticeProse"
      },
      "skos:notation": "RAWFISA_00001",
      "skos:prefLabel": "FBI RAWFISA: This report includes information from FBI FISA collection that has not been evaluated in accordance with minimization procedures (raw FISA-acquired information). FBI personnel with authorized access to raw FISA-acquired information may review, translate, copy, transcribe, analyze, summarize, and use the information contained herein only in accordance with relevant minimization procedures and internal FBI policies. If the raw FISA-acquired information contained herein is not accompanied by handling instructions, then the recipient, following his/her review of the information, must either return the information to the sender of the information or destroy the information, unless the recipient determines the information meets the minimization standard. Once raw FISA-acquired information, and any information derived therefrom, has been minimized by FBI personnel, such minimized FISA-acquired information may only be used in a U.S. legal or administrative proceeding with the advance authorization of the Attorney General, and in coordination with FBI Headquarters."
    },
    {
      "@id": "ismnoticeprose:RC_Dissemination_Control_Required_00001",
      "@type": "skos:Concept",
      "skos:inScheme": {
        "@id": "ismnoticeprose:CVEnumISMNoticeProse"
      },
      "skos:notation": "RC_Dissemination_Control_Required_00001",
      "skos:prefLabel": "This product was collected in a restricted collection mode. Redaction, chipping and release determination shall not result in more permissive release than NOFORN regardless of the Security Indicator Codes (SIC) associated with the product."
    },
    {
      "@id": "ismnoticeprose:RD_00001",
      "@type": "skos:Concept",
      "skos:inScheme": {
        "@id": "ismnoticeprose:CVEnumISMNoticeProse"
      },
      "skos:notation": "RD_00001",
      "skos:prefLabel": "RESTRICTED DATA: This document contains Restricted Data as defined in the Atomic Energy Act of 1954, as amended. Unauthorized disclosure is subject to Administrative and Criminal Sanctions."
    },
    {
      "@id": "ismnoticeprose:SSI_00001",
      "@type": "skos:Concept",
      "skos:inScheme": {
        "@id": "ismnoticeprose:CVEnumISMNoticeProse"
      },
      "skos:notation": "SSI_00001",
      "skos:prefLabel": "Warning: This record contains Sensitive Security Information that is controlled under 49 CFR parts 15 and 1520. No part of this record may be disclosed to persons without a \"need-to-know,\" as defined in 49 CFR parts 15 and 1520, except with the written permission of the Administrator of the Transportation Security Administration or the Secretary of Transportation. Unauthorized release may result in civil penalty or other action. For U.S. government agencies, public disclosure is governed by 5 USC 552 and 49 CFR parts 15 and 1520."
    }
